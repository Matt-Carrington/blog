<!DOCTYPE html>
<html>
<head>
  <title>Rewsly</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div>
	<h1>
		<%= link_to 'Rewsly Logo', root_path %>
	</h1>
</div>

<div>
	<nav>
		<ul>
			<li>
				<%= link_to 'Log in', root_path %>
			</li>
			<li>
				<%= link_to 'Sign up', root_path %>
			</li>
		</ul>
	</nav>
</div>

<div>
	<%= yield %>
</div>

</body>
</html>
